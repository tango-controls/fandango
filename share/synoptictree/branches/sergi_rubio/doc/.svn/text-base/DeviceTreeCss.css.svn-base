body
{
	color: #000000;
	background-color: #F0F0FF;
	font-size: 14px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	text-align: left;
}

h1
{
	color: #F00050;
	font-size: 24px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-weight: bold;
	text-align: center;
	border-top: double black;
	border-right: double black;
	border-bottom: double black;
	border-left: double black
}

h2
{
	color: #8000AA;
	font-size: 20px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	text-decoration: underline;
	text-align: left;
}

h3
{
	color: #8000AA;
	font-size: 18px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-style: italic;
	text-align: left;
	text-indent: 3em
}

td
{
	color: #000000;
	background-color: #F0F0FF;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	text-align: left;
}

a:visited
{
	color: #00AABB
}

a:link
{
	color: #0000FF
}
